<library name="Event_summer">
    <prop-group name="default">
	    <prop name="Billboard">
            <mesh file="bilboard.3ds">
                <texture name="default" diffuse-map="shit_0.jpg"/>
            </mesh>
        </prop>
		<prop name="Slope WC">
			<mesh  file="Slope_1.3DS">
			    <texture name="Slope WC" diffuse-map="slope_1.jpg" />
				<texture name="Slope WC2" diffuse-map="slope_2.jpg" />
			</mesh>
		</prop>
		<prop name="Slope 2">
				<mesh  file="slope_2.3DS">
					<texture name="Slope 21" diffuse-map="slope_21.jpg" />
					<texture name="Slope 22" diffuse-map="slope_22.jpg" />
					<texture name="Slope 23" diffuse-map="slope_23.jpg" />
				</mesh>
		</prop>
		<prop name="Outer Wall 2">
            <mesh file="ow_2.3ds">
                <texture name="Ow2 1" diffuse-map="ow_2.jpg"/>
				<texture name="Ow2 2" diffuse-map="ow_21.jpg"/>
            </mesh>
        </prop>
	</prop-group>

	<prop-group name="Sprites">
        <prop name="Troll">
			<sprite file="troll.png" origin-y="0.95" scale="2"/>
		</prop>
		<prop name="Alien">
			<sprite file="alien.png" origin-y="0.99" scale="3"/>
		</prop>
		<prop name="Bush 1">
            <sprite file="bush1.png" origin-y="0.99" scale="2.5"/>
        </prop>
    </prop-group>

</library>
